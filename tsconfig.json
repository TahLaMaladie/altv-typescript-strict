{
    "compilerOptions": {
	"rootDir": "src",
	"outDir": "dist",
	"module": "ESNext",
	"target": "ESNext",
	"moduleResolution": "node",
    "typeRoots": ["./node_modules/@types", "./node_modules/@altv"],
	"strict": true,
	"noFallthroughCasesInSwitch": true,
	"noImplicitReturns": true,
	"noUncheckedIndexedAccess": true,
	"noUnusedLocals": true,
	"noUnusedParameters": true,
  }
}
